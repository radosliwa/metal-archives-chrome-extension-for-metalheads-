{
    "name": "metal-archives-search",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --port 5000",
        "build-app": "vue-tsc --noEmit && vite build",
        "build-cs": "npx tsc -p tsconfig.node.json --skipLibCheck ",
        "build": "rm -rf dist && npm run build-app && npm run build-cs",
        "preview": "vite preview",
        "lint": "eslint -c .eslintrc.js \"**/*.{vue,ts,js}\"",
        "lint:fix": "eslint -c .eslintrc.js \"**/*.{vue,ts,js}\" --fix",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint:fix && yarn test"
        }
    },
    "dependencies": {
        "@types/es6-promise": "^3.3.0",
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.179",
        "@types/es6-shim": "^0.31.42",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.14.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-istanbul": "^0.27.3",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@vue/test-utils": "^2.2.7",
        "@vueuse/core": "^9.1.1",
        "autoprefixer": "^10.4.7",
        "eslint": "^8.16.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-vue": "^9.0.1",
        "husky": "^8.0.3",
        "jsdom": "^21.1.0",
        "path": "^0.12.7",
        "postcss": "^8.4.13",
        "sass": "^1.51.0",
        "tailwindcss": "^3.0.24",
        "typescript": "^4.7.3",
        "vite": "latest",
        "vite-svg-loader": "^3.4.0",
        "vitest": "^0.28.3",
        "vue-tsc": "^0.34.7"
    }
}
